int main()
{ 
    
    double c,d=2,e,A;
    cin>>A;
    e=2*2*2;
    A=pow(A,e);
    
    
    cout<<A;
    
    return 0;
}
N7
#include <iostream>
#include <math.h>
using namespace std;

int main()
{
setlocale(0, "rus");
double A, t, k ;
cout « "ВВедите значение А, для вычисления А^15: \n";
cout « "\tA = ";
cin » A;
k = A;
t = A * A;
t = t * t;
t = t * t;
t = t * t;
A = t * pow(k, -1);
cout « "A^15 = " « A;
return 0;
}
